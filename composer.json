{
    "name": "aradsolution/agent",
    "description": "Desktop/mobile user agent parser with support for Laravel, based on Mobiledetect",
    "keywords": ["laravel", "useragent", "agent", "user agent", "browser", "platform", "mobile", "desktop"],
    "homepage": "https://github.com/aradsolution/agent",
    "license" : "MIT",
    "authors": [
        {
            "name": "Arad Solution",
            "email": "info@aradsolution.com"
        },
        {
            "name": "Vahid Farid",
            "email": "vahidfarid@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "mobiledetect/mobiledetectlib": "^4.8",
        "jaybizzle/crawler-detect": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "php-coveralls/php-coveralls": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "AradSolution\\Agent\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "AradSolution\\Agent\\AgentServiceProvider"
            ],
            "aliases": {
                "Agent": "AradSolution\\Agent\\Facades\\Agent"
            }
        }
    },
    "suggest": {
        "illuminate/support": "Required for laravel service providers"
    }
}
